{
  "name": "ulianov.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./ --color --fix",
    "prepare": "husky intall",
    "prettier": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "start": "next start"
  },
  "dependencies": {
    "@stitches/react": "0.1.0-canary.20",
    "next": "10.0.8",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@types/node": "14.14.34",
    "@types/react": "17.0.3",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.17.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.3.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-sonarjs": "0.6.0",
    "eslint-plugin-testing-library": "3.10.1",
    "husky": "5.1.3",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "typescript": "4.2.3"
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx}": [
      "eslint --color --fix --quiet",
      "prettier --write --config ./.prettierrc",
      "git add"
    ]
  }
}
